{
  "name": "@floating-ui/utils",
  "version": "0.1.0",
  "@rollingversions": {
    "baseVersion": [
      0,
      1,
      0
    ]
  },
  "description": "Utilities for Floating UI",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/floating-ui.utils.cjs.js",
  "module": "./dist/floating-ui.utils.esm.js",
  "types": "./dist/types.d.ts",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./dom/package.json": "./dom/package.json",
    "./react/package.json": "./react/package.json",
    ".": {
      "types": "./dist/types.d.ts",
      "module": "./dist/floating-ui.utils.esm.js",
      "require": "./dist/floating-ui.utils.cjs.js"
    },
    "./dom": {
      "types": "./dist/dom/index.d.ts",
      "module": "./dist/floating-ui.utils.dom.esm.js",
      "require": "./dist/floating-ui.utils.dom.cjs.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "module": "./dist/floating-ui.utils.react.esm.js",
      "require": "./dist/floating-ui.utils.react.cjs.js"
    }
  },
  "scripts": {
    "test": "vitest --globals",
    "dev": "rollup -c -w",
    "build": "vite build"
  },
  "author": "atomiks",
  "license": "MIT",
  "bugs": "https://github.com/floating-ui/floating-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/floating-ui/floating-ui.git",
    "directory": "packages/utils"
  },
  "homepage": "https://floating-ui.com",
  "keywords": [
    "tooltip",
    "popover",
    "dropdown",
    "menu",
    "popup",
    "positioning"
  ]
}
