    "@testing-library/jest-dom": "^5.17.0",
    "babel-preset-solid": "^1.6.10",
    "jsdom": "^22.1.0",
    "rollup-preset-solid": "^2.0.1",
    "solid-js": "^1.7.4",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.34.1"
  }
