{
  "name": "@floating-ui/solid",
  "version": "0.1.0",
  "@rollingversions": {
    "baseVersion": [
      0,
      1,
      0
    ]
  },
  "description": "Floating UI for SolidJS",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/floating-ui.solid.umd.js",
  "module": "./dist/floating-ui.solid.esm.js",
  "unpkg": "./dist/floating-ui.solid.umd.min.js",
  "types": "./src/types.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./src/types.d.ts",
