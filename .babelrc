{
    "presets": ["stage-2"],
    "plugins": [
        "external-helpers",
        ["module-alias", [
            { "src": "./src/popper/index.js", "expose": "popper.js" }
        ]]
    ]
    "ignore": ["node_modules/**"],
    "env": {
        "test": {
            "plugins": [ "istanbul" ]
        },
        "development": {
            "plugins": [ "istanbul" ]
        }
    }
}
